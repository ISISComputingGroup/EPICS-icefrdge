    pattern {P, STREAM_PV, PV_NAME}

    {"\$(P)", "MIMIC:PV1", "MIMIC:PV2"}
    {"\$(P)", "MIMIC:PV1", "MIMIC:PV4"}
    {"\$(P)", "MIMIC:PV1", "MIMIC:NV"}
}
