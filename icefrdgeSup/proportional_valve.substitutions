file proportional_valve_template.template { 
  pattern 
    {P, PORT, VALVE_NUM, IF_ONE}
    
    {"\$(P)", "\$(PORT)", "1", ""}
    {"\$(P)", "\$(PORT)", "2", "#"}
    {"\$(P)", "\$(PORT)", "4", "#"}
}

