######### Simulation SECTION ##############

record(bo, "$(P)SIM") 
{
	field(DESC, "For the recsim")
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
	field(VAL, "$(RECSIM=0)")	
}

record(bo, "$(P)DISABLE")
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(ai, "$(P)AUTO:TEMP")
{
	field(DESC, "Temperature of the dilution fridge")
	field(DTYP, "stream")
    field(INP, "@icefrdge.proto getAutoTemperature $(PORT)")
	field(SCAN, "1 second")
	field(EGU, "K")
	
	field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:AUTO:TEMP")
    field(SDIS, "$(P)DISABLE")

    info(INTEREST, "HIGH")
	info(archive, "VAL")
}

