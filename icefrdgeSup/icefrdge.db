	field(EGU, "K")

	field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:LS:MC:TEMP:SP")
    field(SDIS, "$(P)DISABLE")

	info(archive, "VAL")
}

record(calc, "$(P)LS:MC:TEMP:SP:_IS_ZERO")
{
	field(CALC, "(A < 0.00001) ? 1 : 2")
	field(FLNK, "$(P)LS:MC:TEMP:SP:_FAN")
}

record(dfanout, "$(P)LS:MC:TEMP:SP:_FAN")
{
	field(DOL, "$(P)LS:MC:TEMP:SP")
	field(OMSL, "closed_loop")
	field(SELL, "$(P)LS:MC:TEMP:SP:_IS_ZERO")
	field(SELM, "Specified")
	field(OUTA, "$(P)LS:MC:TEMP:SP:_IF_ZERO PP")
	field(OUTB, "$(P)LS:MC:TEMP:SP:_IF_NOT_ZERO PP")
}

