	field(TWST, "100 uA")
	field(THST, "316 uA")
	field(FRST, "1.00 mA")
	field(FVST, "3.16 mA")
	field(SXST, "10 mA")
	field(SVST, "31.6 mA")
	field(EIST, "100 mA")

	info(archive, "VAL")
}

